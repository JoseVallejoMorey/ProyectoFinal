<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- PANEL FORM -->
<div class="text-center" style="padding: 50px 0">
	<div class="logo">
		<img alt="logo" src="img/logo.png">
	</div>
	<div class="container">
		<div class="panel panel-danger">
			<div class="panel-heading">Busqueda de contactos</div>
			<div class="panel-body">
				<!-- Main Form -->
				<form id="buscador">
					<div class="col-md-12 col-sm-12">
						<div class="form-group col-md-3 col-sm-3">
							<div class="panel panel-default">
								<div class="panel-body">
									<label for="nombre">Nombre</label> <input type="text"
										class="form-control input-sm" id="nombre" placeholder=""
										name="nombre">
								</div>
							</div>
						</div>

						<div class="form-group col-md-3 col-sm-3">
							<div class="panel panel-default">
								<div class="panel-body">
									<label for="apellido">Apellido</label> <input type="text"
										class="form-control input-sm" id="apellido" placeholder=""
										name="apellido">
								</div>
							</div>
						</div>

						<div class="form-group col-md-3 col-sm-3">
							<div class="panel panel-default">
								<div class="panel-body">
									<label for="empresa">Empresa</label> <input type="text"
										class="form-control input-sm" id="empresa" placeholder=""
										name="empresa">
								</div>
							</div>
						</div>

						<div class="form-group col-md-3 col-sm-3">
							<div class="panel panel-default">
								<div class="panel-body">
									<label for="telefono">Telefono</label> <input type="text"
										class="form-control input-sm" id="telefono" placeholder=""
										name="telefono">
								</div>
							</div>
						</div>
						<div id="ancla" class="col-md-9">
							<div id="ancla" class="sujerencia-response sujerencia-off"></div>
						</div>
				</form>
			</div>
		</div>

		<!-- end:Main Form -->
	</div>